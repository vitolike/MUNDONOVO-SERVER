public OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
	motor[playerid] = 0;
	NovaCamera[playerid] = 0;
	for(new i = 0; i < sizeof(CarInfo); i++)
   	{
     	new string[128];
     	if(vehicleid == CarInfo[i][ownedvehicle])
     	{
   			if(CarInfo[i][cOwned]==0)
		    {
      			CarOffered[playerid]=1;
               	SendClientMessage(playerid, COLOR_LIGHTBLUE, "{FFFFFF}Você pagou {FF0000}R$1000{FFFFFF} para entrar no veiculo de: Concessionária");
			    format(string,sizeof(string),"~w~Carro: %s~n~Preco: ~g~%d~n~~w~use:/comprarcarro",CarInfo[i][cDescription],CarInfo[i][cValue]);
				GameTextForPlayer(playerid,string, 5000, 5);
				CarInfo[i][cGrana] += 1000;
               	GivePlayerMoney(playerid, -1000);
   			}
		  	if(CarInfo[i][cOwned]==1)
		   	{
		    	new playername[MAX_PLAYER_NAME];
		    	GetPlayerName(playerid, playername, sizeof(playername));
			   	if(strcmp(playername,CarInfo[i][cOwner],true)==0)
			    {
					format(string,sizeof(string),"{FFFFFF}Você entrou no seu Veiculo, e nele contem {00EE00}R$%d{FFFFFF} em seu interior",CarInfo[i][cGrana]);
					SendClientMessage(playerid, COLORGZ_EXE, string);
					format(string,sizeof(string),"{FFFFFF}Use: {FFFF00}/retirargrana{FFFFFF} para sacar os {00EE00}R$%d{FFFFFF}. ",CarInfo[i][cGrana]);
					SendClientMessage(playerid, COLORGZ_EXE, string);
				}
				else
				{
					format(string,sizeof(string),"{FFFFFF}Este veículo pertence a {FFFF00}%s",CarInfo[i][cOwner]);
                    SendClientMessage(playerid, COLORGZ_EXE, string);
   					format(string,sizeof(string),"{FFFFFF}Você pagou {00EE00}R$1200{FFFFFF} para entrar no veiculo de: {FFFF00}%s",CarInfo[i][cOwner]);
   					SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
					CarInfo[i][cGrana] += 1200;
                    GivePlayerMoney(playerid, -1200);
				}
    		}
		}
	}
	return true;
}

